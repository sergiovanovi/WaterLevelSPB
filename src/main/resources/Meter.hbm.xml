<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sergiovanovi.model.Meter" table="meter" schema="test">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="meter">
            <column name="METER" sql-type="double" precision="-1"/>
        </property>
        <property name="date">
            <column name="DATE" sql-type="timestamp(4)"/>
        </property>
    </class>
</hibernate-mapping>