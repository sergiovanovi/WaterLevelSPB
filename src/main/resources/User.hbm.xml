<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sergiovanovi.model.User" table="mail" schema="test">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="mail">
            <column name="MAIL" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="min">
            <column name="MIN" sql-type="int(11)"/>
        </property>
        <property name="max">
            <column name="MAX" sql-type="int(11)"/>
        </property>
        <property name="util">
            <column name="UTIL" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>